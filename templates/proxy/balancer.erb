
  <Proxy balancer://<%= @balancername %>>
    <%- @members.each do | key, value | -%>
    BalancerMember <%= key %>
    <%- end -%>
    <%- if scope.function_versioncmp([@version, '2.4']) >= 0 -%>
      <%- if defined?(@lbmethod) -%>
    ProxySet lbmethod=<%= @lbmethod %>
      <%- end -%>
    <%- end -%>
  </Proxy>
