
  ProxyPass <%= @url %> <%= @destination -%>
  <% if defined?(@connectiontimeout) %> connectiontimeout=<%= @connectiontimeout %><% end -%>
  <% if defined?(@timeout) %> timeout=<%= @timeout %><% end -%>
  <% if defined?(@disablereuse) %> disablereuse=<%= scope.function_bool2onoff([@disablereuse]) %><% end -%>
